{
    "name": "zealouswebcraftcms/smart-google-analytics",
    "description": "Smart Google Analytics",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "Smart Google Analytics"
    ],
    "support": {
        "docs": "https://github.com/zealouswebcraftcms/smart-google-analytics/blob/master/README.md",
        "issues": "https://github.com/zealouswebcraftcms/smart-google-analytics/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "zealousweb",
            "homepage": "https://www.zealousweb.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "google/apiclient": "^v2.6.0"
