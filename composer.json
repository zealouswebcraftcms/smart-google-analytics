        "developerUrl": "https://www.zealousweb.com",
        "documentationUrl": "https://github.com/zealouswebcraftcms/smart-google-analytics/blob/master/README.md",
        "changelogUrl": "https://github.com/zealouswebcraftcms/smart-google-analytics/blob/master/CHANGELOG.md",
        "class": "zealouswebcraftcms\\smartgoogleanalytics\\SmartGoogleAnalytics"
